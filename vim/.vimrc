colorscheme gruvbox
set background=dark
set number
set nocompatible
set hidden
set noerrorbells
filetype on
filetype plugin on
filetype indent on
syntax on
set cursorline
set signcolumn=yes
set shiftwidth=4
set tabstop=4
set expandtab
set nowrap
set incsearch
set showmatch
set hlsearch
set showcmd
set wildmenu

" Keybinds
let mapleader= " "
nmap <tab> :NERDTreeToggle<CR>
